<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#434343"
    tools:context=".SignUp">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="523dp"
        android:layout_height="915dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/background" />

    <TextView
        android:id="@+id/su_tv_v_error"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="ERROR"
        android:textColor="#DC0A0A"
        android:textSize="16sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/su_iv_v_cPassword" />

    <TextView
        android:id="@+id/su_tv_v_success"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="SUCCESS"
        android:textColor="#EDCB4B"
        android:textSize="16sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/su_tv_v_error" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:layout_marginBottom="173dp"
        app:layout_constraintBottom_toTopOf="@+id/su_et_v_uname"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/app_name" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:srcCompat="@drawable/sign_up" />

    <ImageView
        android:id="@+id/su_iv_v_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:srcCompat="@drawable/text_box_rectangle" />

    <ImageView
        android:id="@+id/su_iv_v_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@+id/su_iv_v_username"
        app:layout_constraintTop_toBottomOf="@+id/su_iv_v_username"
        app:srcCompat="@drawable/text_box_rectangle" />

    <ImageView
        android:id="@+id/su_iv_v_cPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="@+id/su_iv_v_password"
        app:layout_constraintTop_toBottomOf="@+id/su_iv_v_password"
        app:srcCompat="@drawable/text_box_rectangle" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="@+id/su_iv_v_password"
        app:layout_constraintTop_toBottomOf="@+id/su_iv_v_password"
        app:srcCompat="@drawable/text_box_rectangle" />

    <ImageView
        android:id="@+id/su_iv_v_submit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        app:layout_constraintStart_toStartOf="@+id/su_iv_v_cPassword"
        app:layout_constraintTop_toBottomOf="@+id/su_iv_v_cPassword"
        app:srcCompat="@drawable/submit_button" />

    <EditText
        android:id="@+id/su_et_v_uname"
        android:layout_width="185dp"
        android:layout_height="41dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="168dp"
        android:ems="10"
        android:fontFamily="@font/nova_round"
        android:inputType="text"
        android:text="Create Username"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/su_iv_v_username"
        app:layout_constraintHorizontal_bias="0.011"
        app:layout_constraintStart_toStartOf="@+id/su_iv_v_username"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

    <EditText
        android:id="@+id/su_et_v_pword"
        android:layout_width="183dp"
        android:layout_height="46dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="1dp"
        android:ems="10"
        android:fontFamily="@font/nova_round"
        android:inputType="text"
        android:text="Create Password"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/su_iv_v_password"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/su_iv_v_password"
        app:layout_constraintTop_toBottomOf="@+id/su_iv_v_username" />

    <EditText
        android:id="@+id/su_et_v_cPword"
        android:layout_width="183dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:fontFamily="@font/nova_round"
        android:inputType="text"
        android:text="Confirm Password"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/su_iv_v_cPassword"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/su_iv_v_cPassword"
        app:layout_constraintTop_toBottomOf="@+id/su_et_v_pword" />

    <ImageView
        android:id="@+id/su_iv_v_back"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_button" />

</androidx.constraintlayout.widget.ConstraintLayout>